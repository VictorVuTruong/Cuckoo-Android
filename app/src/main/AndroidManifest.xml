<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.beta.myhbt_api">
    <!-- Internet Permission -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/hbtgram1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/hbtgram1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.SignUpCreateEmailAndPassword"></activity>
        <activity android:name=".View.SignUpVerifyInfo" />
        <activity android:name=".View.SignUpEnterStudentId" />
        <activity
            android:name=".View.MainMenu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".View.HBTGramPostDetail" />
        <activity android:name=".View.HBTGram" />
        <activity android:name=".View.MainPage" />
        <activity android:name=".View.LoginActivity" />
        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>